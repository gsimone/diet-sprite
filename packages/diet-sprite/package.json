{
  "name": "diet-sprite",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./filters": {
      "import": {
        "types": "./dist/filters.d.ts",
        "default": "./dist/filters.js"
      },
      "require": {
        "types": "./dist/filters.d.ts",
        "default": "./dist/filters.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown && tsc --project tsconfig.build.json",
    "dev": "tsdown --watch & tsc --project tsconfig.build.json --watch --preserveWatchOutput"
  },
  "peerDependencies": {
    "three": "^0.139.2"
  },
  "dependencies": {
    "earcut": "^2.2.3"
  },
  "devDependencies": {
    "tsdown": "^0.2.0",
    "typescript": "^5.5.2"
  }
}
